{
    "editor.formatOnSave": true,
    "files.autoSave": "onFocusChange",
    "files.trimFinalNewlines": true,
    "notebook.insertFinalNewline": false,
    "files.insertFinalNewline": true,
    "files.trimTrailingWhitespace": true,
    "files.eol": "\n",
    "cpputestTestAdapter.preLaunchTask": "build_for_unittest",
    "cpputestTestAdapter.testExecutable": "${workspaceFolder}/.build_test/tests/UnitTests",
    "cpputestTestAdapter.testExecutablePath": "${workspaceFolder}/.build_test/tests",
    "cpputestTestAdapter.debugLaunchConfigurationName": "",
    "cmake.automaticReconfigure": false,
    "cmake.ctest.testExplorerIntegrationEnabled": false,
    "cmake.saveBeforeBuild": false,
    "cpputestTestAdapter.testLocationFetchMode": "auto",
    "cmake.buildBeforeRun": true,
    "cmake.buildTask": true,
    "cmake.configureArgs": [
        "-GNinja",
        "-DBUILD_FOR_UNIT_TESTS=ON"
    ],
    "cpputestTestAdapter.logpanel": false,
    "cortex-debug.liveWatchRefreshRate": 500,
    "cortex-debug.variableUseNaturalFormat": false,
    "cortex-debug.armToolchainPath": "/usr/bin",
    "cortex-debug.gdbPath": "/usr/bin/gdb",
    "cortex-debug.pyocdPath": "/usr/bin/pyocd",
    "cpputestTestAdapter.objDumpExecutable": "",
}
